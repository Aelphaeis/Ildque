<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Ildque</display-name>

	<listener>
		<listener-class>com.cruat.ildque.init.SystemIntegrityListener</listener-class>
	</listener>
	<listener>
		<listener-class>com.cruat.ildque.init.BotInitListener</listener-class>
	</listener>

	<resource-env-ref>	
		<description>t</description>
		<resource-env-ref-name>ildque/token</resource-env-ref-name>
		<resource-env-ref-type>com.cruat.ildque.config.BotToken</resource-env-ref-type>
	</resource-env-ref>
</web-app>
