<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<listener>
		<listener-class>com.cruat.ildque.init.SystemIntegrityListener</listener-class>
	</listener>
	<listener>
		<listener-class>com.cruat.ildque.init.BotInitListener</listener-class>
	</listener>

	<resource-ref>
		<description>login token</description>
		<res-ref-name>ildque/token</res-ref-name>
		<res-type>com.cruat.ildque.config</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
</web-app>
